  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DNK</connection>
    <sql>SELECT A. *,
       COSTO *CANTIDAD AS COSTO_TOTAL,
		producto_precio_de_venta *CANTIDAD AS COSTO_VENTA
FROM (SELECT mw.name AS nombreAlmacen,
             TO_DATE (NOW ()) AS fecha,
             date_part (&apos;day&apos;,NOW ()) AS dia,
             date_part (&apos;month&apos;,NOW ()) AS mes,
             date_part (&apos;year&apos;,NOW ()) AS anio,
             date_part (&apos;quarter&apos;,NOW ()) AS trimestre,
             CASE
               WHEN date_part (&apos;quarter&apos;,NOW ()) IN (1,2) THEN 1
               ELSE 2
             END AS semestre,
             sd.qtyonhand cantidad,
             IO.movementdate AS producto_fecha_ultima_entrada,
             IO.movementqty AS producto_cantidad_ultima_entrada,
			PL.pricelist AS producto_precio_de_venta,
             ZNK4_SACACOSTO (sd.M_PRODUCT_ID,TO_DATE (NOW ())) AS COSTO,
      ------------------
      --CL.C_LOCATION_ID       
      CL.address1 AS almacen_calle,
      CL.address1 AS almacen_colonia,
      CL.city AS almacen_ciudad,
      CL.postal AS almacen_cp,
      CL.regionname AS almacen_estado,
      cty.name AS almacen_pais,
      ------------------
      P.m_product_id AS id_producto,
             P.name AS nombre_producto,
             PC.name AS categoria_producto,
             MARCA.NAME AS MARCA,
             ASI.description AS producto_descripcion,
             
      --P.EM_ZNK2_MARCA_ID,
      TRIM (P.em_znk2_tipo_producto) AS tipo,
             P.EM_ZNK2_GENERO,
             P.EM_ZNK2_ATM as ATM,
             P.EM_ZNK2_BASICO_NOVEDAD,
             P.EM_ZNK2_STATUS,
             P.EM_ZNK2_TEMPORADA,
             EM_ZNK2_ANO_TEMPORADA,
             EM_ZNK2_HAY_BRILLANTES,
             P.EM_ZNK2_MAQUINARIA,
             P.EM_ZNK2_MOVIMIENTO,
             ZNK2_FORMA_CAJA.NAME AS ZNK2_FORMA_CAJA,
             EM_ZNK2_JOYAS_TAMANO,
             EM_ZNK2_JOYERIA_CLASIFICACION,
             ZNK2_MATERIAL_CAJA.NAME AS MATERIAL_CAJA,
             ZNK2_MATERIAL_CORREA.NAME AS MATERIAL_CORREA,
             ZNK2_TERMINADO_CAJA.NAME AS TERMINADO_CAJA,
             ZNK2_TERMINADO_CORREA.NAME AS TERMINADO_CORREA,
             ZNK2_COLOR_CAJA.NAME AS COLOR_CAJA,
             ZNK2_COLOR_CORREA.NAME AS COLOR_CORREA,
             ZNK2_COLOR_CARATULA.NAME AS COLOR_CARATULA,
             ZNK2_TERMINADO_CARATULA.NAME AS TERMINADO_CARATULA,
             ZNK2_CLASIFICACION.NAME AS CLASIFICACION,
             ZNK2_NOMBRE_ESTILO.NAME AS NOMBRE_ESTILO,
             ZNK2_JOYAS_METALES.NAME AS JOYAS_METALES,
             ZNK2_JOYAS_ADITAMENTOS.NAME AS JOYAS_ADITAMENTOS,
             ZNK2_JOYAS_COLOR.NAME AS JOYAS_COLOR,
             ZNK2_JOYAS_MATERIALES.NAME AS JOYAS_MATERIALES,
             ZNK2_JOYAS_FORMA.NAME AS JOYAS_FORMA,
             FCOLLAR.NAME AS FORMACOLLAR,
             FBRASALETE.NAME AS FORMABRASALETE,
             FARETES.NAME AS FORMAARETES,
             FANILLO.NAME AS FORMAANILLO,
             FBOXSET.NAME AS FORMABOXSET,
             FCHARM.NAME AS FORMACHARM,
             ZNK2_COMBINACION_BOXSET.NAME AS COMBINACION_BOXSET,
             COLORADITAMENTO.NAME AS COLOR_ADITAMENTO
      FROM m_storage_detail sd 
        INNER JOIN m_locator ml ON sd.m_locator_id = ml.m_locator_id AND ml.ad_client_id = &apos;2D4B4E8A2ACE7F47012AD3387996000A&apos; 
        INNER JOIN m_warehouse mw ON mw.m_warehouse_id = ml.m_warehouse_id 
        INNER JOIN m_product P ON P.m_product_id = sd.m_product_id
        INNER JOIN c_location cl ON mw.c_location_id = CL.c_location_id
      --Joins de pzas en ultima entrada
      --Ultima fecha de entrada
      
        INNER JOIN(
			SELECT (SELECT MT1.movementdate
       FROM m_transaction MT1
       WHERE TRIM(MT1.movementtype) LIKE &apos;V+&apos;
       AND   Mt1.movementdate &lt;= TO_DATE (NOW ())
       AND   MT1.m_locator_id = MT3.m_locator_id
       AND   MT1.m_product_id = MT3.m_product_id
       AND   MT1.m_attributesetinstance_id = MT3.m_attributesetinstance_id
       ORDER BY MT1.movementdate DESC LIMIT 1)
AS movementdate,
       (SELECT MT2.movementqty
        FROM m_transaction MT2
        WHERE TRIM(MT2.movementtype) LIKE &apos;V+&apos;
        AND   Mt2.movementdate &lt;= TO_DATE (NOW ())
        AND   MT2.m_locator_id = MT3.m_locator_id
        AND   MT2.m_product_id = MT3.m_product_id
        AND   MT2.m_attributesetinstance_id = MT3.m_attributesetinstance_id
        ORDER BY MT2.movementdate DESC LIMIT 1)
AS movementqty,
       MT3.m_locator_id,
       MT3.m_product_id,
       MT3.m_attributesetinstance_id
FROM (SELECT DISTINCT m_locator_id,m_product_id,m_attributesetinstance_id FROM m_transaction WHERE TRIM(movementtype) LIKE &apos;V+&apos;  AND movementdate &lt;= TO_DATE (NOW ())) MT3
GROUP BY MT3.m_locator_id,
         MT3.m_product_id,
         MT3.m_attributesetinstance_id) IO ON IO.m_product_id = sd.m_product_id AND IO.m_locator_id=sd.m_locator_id AND IO.m_attributesetinstance_id = sd.m_attributesetinstance_id AND IO.movementqty &gt; 0
      --Fin DE Join
	--Pricelist
	LEFT JOIN (SELECT PP.m_product_id,PP.pricelist,PP.ad_client_id FROM m_productprice PP
			LEFT JOIN m_pricelist_version PLV ON PP.m_pricelist_version_id = PLV.m_pricelist_version_id AND PLV.isactive = &apos;Y&apos;
			INNER JOIN m_pricelist PL ON PL.m_pricelist_id = PLV.m_pricelist_id 
			AND PL.isactive=&apos;Y&apos; AND isdefault=&apos;Y&apos; AND issopricelist=&apos;Y&apos; AND c_currency_id=&apos;130&apos;) PL ON PL.m_product_id = sd.m_product_id AND PL.ad_client_id=&apos;2D4B4E8A2ACE7F47012AD3387996000A&apos;

	--Fin de Pricelist
        LEFT JOIN c_country cty ON cl.c_country_id = cty.c_country_id
        LEFT JOIN ZNK2_FORMA_CAJA ON EM_ZNK2_FORMA_CAJA_ID = ZNK2_FORMA_CAJA_ID 
        LEFT JOIN ZNK2_MATERIAL_CAJA ON EM_ZNK2_MATERIAL_CAJA_ID = ZNK2_MATERIAL_CAJA_ID 
        LEFT JOIN ZNK2_MATERIAL_CORREA ON EM_ZNK2_MATERIAL_CORREA_ID = ZNK2_MATERIAL_CORREA_ID 
