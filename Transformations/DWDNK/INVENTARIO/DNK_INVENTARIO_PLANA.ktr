    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here

&#47;&#47;Estados
if(almacen_estado == null){
	a=&quot;NO DEFINIDO&quot;;
}else if(almacen_estado.match(&#47;(nuevo leon|n.l.|n.l|nl.|nl)&#47;i)){
	a=&quot;NUEVO LEON&quot;;
}else if(almacen_estado.match(&#47;(ver|ver.|veracruz)&#47;i)){
	a=&quot;VERACRUZ&quot;;
}else{
	a=almacen_estado.toUpperCase();
}

&#47;&#47;Ciudades
if(almacen_ciudad == null){
	b=&quot;NO DEFINIDO&quot;;
}else if(almacen_ciudad.match(&#47;(mty.|mty|monterrey|monterey)&#47;i)){
	b=&quot;MONTERREY&quot;;
}else if(almacen_ciudad.match(&#47;(ver|ver.|veracruz)&#47;i)){
	b=&quot;VERACRUZ&quot;;
}else{
	b=almacen_ciudad.toUpperCase();
}</jsScript_script>
